@model MetaGraffiti.Web.Admin.Models.AdminViewModel
<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>@Model.PageTitle | MetaGraffiti</title>
	<script src="https://maps.googleapis.com/maps/api/js?key=@(AutoConfig.GoogleMapsApiKey)&libraries=places"></script>
	@System.Web.Optimization.Scripts.Render("~/bundle/js")
	@System.Web.Optimization.Styles.Render("~/bundle/css")
</head>
<body>
	<div class="container-fluid">
		@RenderBody()
	</div>
</body>
</html>