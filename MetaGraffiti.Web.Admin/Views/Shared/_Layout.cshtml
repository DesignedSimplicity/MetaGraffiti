@using MetaGraffiti.Web.Admin.Models;
@model AdminViewModel
<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>@Model.PageTitle@(String.IsNullOrWhiteSpace(Model.PageTitle) ? "" : " | ")MetaGraffiti</title>
	<script src="https://maps.googleapis.com/maps/api/js?key=@(AutoConfig.GoogleMapsApiKey)&libraries=places"></script>
	<script defer src="https://use.fontawesome.com/releases/v5.0.13/js/all.js" crossorigin="anonymous"></script>
	@System.Web.Optimization.Scripts.Render("~/bundle/js")
	@System.Web.Optimization.Styles.Render("~/bundle/css")
</head>
<body>
	<div class="container-fluid">
		@RenderBody()
	</div>
</body>
</html>